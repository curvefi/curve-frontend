{
  "name": "@curvefi/primitives",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"**/*.ts*\"",
    "typecheck": "tsc --noEmit --tsBuildInfoFile ../../.cache/typecheck/packages-primitives.tsbuildinfo"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "devDependencies": {
    "eslint": "*",
    "eslint-config-custom": "*",
    "typescript": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  }
}
