{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "dist",
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmit": false,
    "paths": {
      "@primitives": [
        "../../primitives/src/index.ts"
      ],
      "@primitives/*": [
        "../../primitives/src/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
